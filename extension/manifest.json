{
  "manifest_version": 2,
  "name": "My Test Extension",
  "version": "0.1",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "peerjs.min.js",
        "content.js"
      ]
    }
  ]
}